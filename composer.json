{
    "name": "sweikenb/php-featureflags",
    "description": "Simple feature-flags management for PHP projects.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Sweikenb\\Library\\FeatureFlags\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweikenb\\Library\\FeatureFlags\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
